import java.util.Scanner;

public class LoopHW {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.print("Please enter your name: ");
		Scanner scanner1 = new Scanner(System.in);
		
		int counter = 1000;
		while (counter > 1) {
			if (counter == 7) {
			System.out.println(counter);
			}
			counter -= 1;
		}
	}
	

}
